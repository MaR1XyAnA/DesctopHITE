<Page 
    x:Class="DesctopHITE.PerformanceFolder.PageFolder.PanelMenuFolder.MenuWorkerPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" 
    IsVisibleChanged="Page_IsVisibleChanged"
    Style="{DynamicResource MenuSpacePageStyle}">
    <StackPanel Style="{DynamicResource OtherMenuStackPanelStyle}">
        <ToggleButton
            ToolTip="Все сотрудники"
            Click="ListWorkweToggleButton_Click"
            x:Name="ListWorkweToggleButton"
            Content="ВСЕ СОТРУДНИКИ"
            Style="{DynamicResource OtherMenuToggleButtonStyle}"/>
        <ToggleButton
            ToolTip="Общая информация"
            Margin="15 0 0 0"
            Click="GeneralInformationWorkerToggleButton_Click"
            x:Name="GeneralInformationWorkerToggleButton"
            Content="ОБЩАЯ ИНФОРМАЦИЯ"
            Style="{DynamicResource OtherMenuToggleButtonStyle}"/>
    </StackPanel>
</Page>

