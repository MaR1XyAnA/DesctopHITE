<Page 
    x:Class="DesctopHITE.PerformanceFolder.PageFolder.SettingsBodyFolder.AboutAppPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" 
    Style="{DynamicResource BodySpacePageStyle}">
    <Grid>
        <StackPanel 
            Margin="10"
            VerticalAlignment="Top"
            HorizontalAlignment="Right">
            <StackPanel 
                Margin="0 0 0 10"
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Компания разработчика:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Text="GoodProgrammer©"
                    Margin="3.5 0 0 0"/>
            </StackPanel>
            <StackPanel
                Margin="0 10 0 10"
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    VerticalAlignment="Top"
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Разработчики:"
                    Margin="0 0 3.5 0"/>
                <StackPanel 
                    VerticalAlignment="Bottom"
                    Margin="3.5 0 0 0"
                    Orientation="Vertical"
                    Style="{DynamicResource HorizontalDataStackPanel}">
                    <StackPanel Margin="0 0 0 3.5" Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Аккаунт:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Продакт:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Проджект-менеджер:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Архитектор:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Бизнес Аналитик:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Системный аналитик:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Проектировщик:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Дизайнер:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Верстальщик:"
                            Margin="0"/>
                        <TextBlock
                            Style="{DynamicResource OtherStyleTextBlock}"
                            Text="Миронов Владислав Дмитриевич"
                            Margin="3.5 0 0 0"/>
                    </StackPanel>
                    <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                        <TextBlock
                            Style="{DynamicResource MainStyleTextBlock}"
                            Text="Разработчик / Программист:"
                            Margin="0"/>
