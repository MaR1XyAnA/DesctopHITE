            </StackPanel>
            <StackPanel
                Margin="0 10 0 10" 
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Платформа:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Margin="3.5 0 0 0"
                    Text="WPF (.NET Framework)"/>
            </StackPanel>
            <StackPanel 
                Margin="0 10 0 10"
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Помощь при разработки:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Margin="3.5 0 0 0"
                    Text="ChatGPT \ GitHub \ Internet"/>
            </StackPanel>
            <StackPanel
                Margin="0 10 0 10" 
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Для платформ:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Margin="3.5 0 0 0"
                    Text="Windows 10 \ 8.1 \ 8 \ 7 \ Vista \ XP"/>
            </StackPanel>
            <StackPanel 
                Margin="0 10 0 10"
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Язык для данных:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Margin="3.5 0 0 0"
                    Text="Русский \ Английский \ Юникод"/>
            </StackPanel>
            <StackPanel 
                Margin="0 10 0 10" 
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Язык:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Margin="3.5 0 0 0"
                    Text="Русский \ Юникод"/>
            </StackPanel>
            <StackPanel 
                Margin="0 10 0 10" 
                Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource MainStyleTextBlock}"
                    Text="Поддержка:"
                    Margin="0 0 3.5 0"/>
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Text="10/09/2022 - 10/09/2025"
                    Margin="3.5 0 0 0"/>
            </StackPanel>
        </StackPanel>
        <TextBlock
            Padding="10"
            x:Name="TitleTextTextBlock"
            TextWrapping="Wrap"
            Width="520"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Margin="10"
            Style="{DynamicResource MainStyleTextBlock}"/>
        <StackPanel 
            Margin="10" 
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom">
            <StackPanel Style="{DynamicResource HorizontalDataStackPanel}">
                <TextBlock
                    Style="{DynamicResource OtherStyleTextBlock}"
                    Text="Россия, Москва, 2023"
                    Margin="3.5 0 0 0"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
