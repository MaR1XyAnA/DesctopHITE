        <StackPanel Style="{DynamicResource Authorization_StackPanel-Style}">
            <Grid>
                <TextBox 
                    Width="400"
                    Style="{DynamicResource Authorization_TextBox-Style}"/>
            </Grid>
        </StackPanel> 
    </Grid>
</Page>
